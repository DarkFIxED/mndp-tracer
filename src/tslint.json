{
  "extends": "../tslint.json",
  "rules": {
    "prefer-const": false,
    "member-ordering": false,
    "indent": [true, "tabs", 2],
    "directive-selector": [
      true,
      "attribute",
      "app",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "app",
      "kebab-case"
    ]
  }
}
